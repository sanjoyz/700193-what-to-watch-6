{
  "name": "what-to-watch",
  "version": "1.0.0",
  "private": true,
  "description": "Личный проект «Что посмотреть» от HTML Academy",
  "scripts": {
    "eslint": "eslint --fix --ext .jsx --ext .js src/",
    "test": "npm run eslint && npm run test.jest",
    "test.jest": "jest",
    "build": "webpack --mode production",
    "start": "webpack-dev-server"
  },
  "devDependencies": {
    "@babel/core": "7.12",
    "@babel/preset-env": "7.12",
    "@babel/preset-react": "7.12",
    "@testing-library/jest-dom": "5.11.10",
    "@testing-library/react": "11.2.5",
    "axios-mock-adapter": "1.19.0",
    "babel-loader": "8.2",
    "eslint": "7.18.0",
    "eslint-config-htmlacademy": "2.1.0",
    "eslint-plugin-react": "7.22",
    "jest": "26.6.3",
    "redux-mock-store": "1.5.4",
    "webpack": "4.46",
    "webpack-cli": "3.3",
    "webpack-dev-server": "3.11"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "dependencies": {
    "axios": "0.21.1",
    "history": "4.10",
    "prop-types": "15.7.2",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-redux": "7.2.2",
    "react-router-dom": "5.2.0",
    "redux": "4.0.5",
    "redux-devtools-extension": "2.13.8",
    "redux-thunk": "2.3.0"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less)$": "<rootDir>/src/style-mock.js"
    },
    "setupFilesAfterEnv": [
      "./setup-tests.js"
    ]
  }
}
